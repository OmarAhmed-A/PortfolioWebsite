    "baseui": "^9.116.3",
    "chart.js": "^2.9.4",
    "gh-pages": "^2.2.0",
    "node-fetch": "^2.7.0",
    "prettier-package-json": "^2.8.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.6.8",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-pdf": "^7.7.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.11",
    "styletron-engine-atomic": "^1.6.2",
    "styletron-react": "^5.2.7"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
